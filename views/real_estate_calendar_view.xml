<odoo>
    <data>
        <record id="real_estate_view_calendar" model="ir.ui.view">
            <field name="name">real_estate calendar</field>
            <field name="model">realestate.properties</field>
            <field name="arch" type="xml">
                <calendar string="Property enlisted" date_start="available_from"
                    date_stop="deadline" event_open_popup="true" mode="month" color="name"
                    quick_add="False">
                    <field name="name" />
                    <field name="status" />
                </calendar>
            </field>
        </record>
    </data>
</odoo>